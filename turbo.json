{
  "$schema": "http://turborepo.org/schema.json",
  "pipeline": {
    "homol": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "buildcomponents": {
      "outputs": [
        "dist/**"
      ]
    },
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    }
  }
}